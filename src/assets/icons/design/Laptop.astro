---
interface Props {
  styles?: string;
}

const { styles } = Astro.props;
---

<svg
  viewBox="0 0 24 24"
  fill="none"
  stroke="#10B981"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={styles}
>
  <rect width="18" height="12" x="3" y="4" rx="2" ry="2"></rect>
  <line x1="2" x2="22" y1="20" y2="20"></line>
</svg>
